@model IEnumerable<Category>

<h1>Category</h1>


<table class="table table-bordered table-striped" style="width:100%">
    <thread>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thread>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.Name
                </td>
                <td>
                    @obj.DisplayOrder
                </td>
            </tr>
        }

    </tbody>

</table>
